 class Solution {
    public String reverseWords(String s) {
        //String ss=s.trim();
      String arr[]=s.split(" ");
        StringBuilder sb=new StringBuilder();
        for(int i=arr.length-1;i>=0;i--){
            if(arr[i]!=""){
         sb=sb.append(arr[i]).append(" ");}

          else{

          }
        }
return sb.toString().trim();
       // return h.trim();
    }
}
