 class Solution {
    public String intToRoman(int num) {
        int val[]={1000,900,500,400,100,90,50,40,10,9,5,4,1};
        String symbol[]={"M","CM","D","CD","C","XC","L","XL","X","IX","V","IV","I"};
        String res="";
       for(int i=0;i<13;i++){
         if(num==0)break;
        
         //if(val[i]<num)
         int time=num/val[i];
        while(time-->0){
         res+=symbol[i];
       // time--;


       }
         
       

       num=num%val[i];}

     return res;
    }
}
